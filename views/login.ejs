  <body class="text-center jlogin">

    <nav class="navbar navbar-expand-md navbar-dark fixed-top" style="background-color: var(--mainColor)">
        <a class="navbar-brand" href="/"><i class="fas fa-map-marked"></i></a>
        <div class="my-2 my-lg-0 ml-auto">
        <a class="btn btn-light mr-sm-2"  href="/register">Register</a>
        </div>
   
      </nav>

    <form class="form-signin" action="/login" method="POST">
      <h1 class="h3 mb-3 font-weight-normal">Login</h1>
      <label for="inputEmail" class="sr-only">Email address</label>
      <input type="email"  class="form-control" placeholder="Email address" name="email" required autofocus>
      <label for="inputPassword" class="sr-only">Password</label>
      <input type="password" class="form-control" placeholder="Password" name="password" required>
      <div class="checkbox mb-3">
        <label>
          <input type="checkbox" value="remember-me"> Remember me
        </label>
      </div>
      <button class="btn btn-lg btn-primary btn-block" type="submit">Login</button>
    </form>
  </body>
</html>